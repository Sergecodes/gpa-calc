addCourseInput = semesterNum => {\n    let numCourses = allCourses.length;\n    let id;\n\n    if (numCourses !== 0)   // if there is a course\n        id = allCourses[numCourses-1].id + 1;  // get unused id by taking id of last course(max id in array) + 1\n    else\n        id = 1;\n\n    setAllCourses(\n      ...allCourses,\n      {\n          semesterNum: semesterNum,\n          id: id,\n          name: '',\n          credit: '',\n          grade: '-'\n      }\n    );\n  }\n\n  const removeCourse = id => {\n    const remainingCourses = allCourses.filter(course => course.id !== id);\n    setAllCourses(remainingCourses);\n  }\n\n  /* Handle modification of the value of a course, course info should be in sync with allCourses.. */\n  const handleCourseInfoChange = modifiedCourse => {\n    const unchangedCourses = allCourses.filter(course => course.id !== courseId);\n    setAllCourses(\n      ...unchangedCourses,\n      modifiedCourse\n    );\n  }\n\n  const addSemester = () => {\n    let numSemesters = semesters.length;\n    let semesterNum;\n\n    if (numSemesters !== 0)\n        semesterNum = semesters[numSemesters-1].number + 1;\n    else\n        semesterNum = 1;\n\n    setSemesters(\n      ...semesters,\n      {\n          number: semesterNum,\n          gpa: '-'\n      }\n    );\n  }\n\n  const removeSemester = num => {\n    const remainingSems = semesters.filter(sem => sem.num !== num);\n    setSemesters(remainingSems);\n  }\n\n  return (\n    <>\n  \t\t<LanguageSelector />\n      <br />\n  \t\t<Heading />\n  \t\t<br />\n  \t\t<main>\n        {semesters.map(\n          semester => (\n            <>\n              <Semester\n                key={semester.number}\n                allCourses={allCourses}\n                addCourseInput={addCourseInput}\n                removeSemester={removeSemester}\n                removeCourse={removeCourse}\n                handleCourseInfoChange={handleCourseInfoChange}\n              />\n              <br />\n            </>\n          )\n        )}\n        <button onClick={addSemester}>Add Semester</button>\n  \t\t</main>\n    </>\n  );\n}\n"]},"metadata":{},"sourceType":"module"}