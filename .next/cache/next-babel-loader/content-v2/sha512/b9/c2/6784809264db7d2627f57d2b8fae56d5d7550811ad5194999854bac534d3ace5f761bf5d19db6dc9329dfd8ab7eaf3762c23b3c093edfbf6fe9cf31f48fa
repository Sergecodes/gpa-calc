;\n    const numCourses = semester.courses.length;\n\n    return (\n      <>\n        <div id={`semester-${semester.number}`}>\n          <div style={{\n            fontVariant: 'small-caps',\n            color: 'rgba(249, 71, 47, .9)',\n            textAlign: 'center',\n            border: '2px solid rgba(123, 123, 123, 0.15)',\n            borderRadius: '4px',\n          }}>\n            <h2 style={{margin: '.2rem auto'}}>Semester {semester.number}</h2>\n          </div>\n          <TableContainer component={Paper}>\n              <Table aria-label={`Semester ${semester.number} form`}>\n                  <TableHead>\n                      <TableRow style={{borderBottom: '1px solid rgb(224, 224, 224)'}}>\n                          <TableCell>Course name</TableCell>\n                          <TableCell>Credit</TableCell>\n                          <TableCell>Marks /100</TableCell>\n                          <TableCell size=\"small\" style={{textAlign: 'center'}}>Grade</TableCell>\n                      </TableRow>\n                  </TableHead>\n\n                  <TableBody>\n                      {semester.courses.map(\n                          course => (\n                            <CourseInput\n                              key={course.id}\n                              course={course}  // pass course to CourseInput component\n                              semester={semester}\n                              onInputChange={onInputChange}\n                              onMarkOrCreditChange={onMarkOrCreditChange}\n                              onDeleteCourse={onDeleteCourse}\n                            />\n                          )\n                      )}\n                      <TableRow className=\"semester-actions\">\n                        <TableCell>\n                          <Button\n                            variant=\"contained\"\n                            color=\"primary\"\n                            onClick={() => onAddCourse(semester)}\n                            startIcon={<AddCircleOutlineIcon />}\n                          >\n                            Add course\n                          </Button>\n                        </TableCell>\n\n                        <TableCell>\n                        </TableCell>\n                        {numSemesters === 1 ?\n                          <>\n                            <TableCell>\n                            </TableCell>\n                            <TableCell style={{paddingLeft: '0'}}>\n                              <Button\n                                variant=\"contained\"\n                                color=\"secondary\"\n                                onClick={() => onResetSemester(semester)}\n                                >\n                                Reset\n                              </Button>\n                            </TableCell>\n                          </>\n                          :\n                          <>\n                            <TableCell style={{\n                              float: 'right',\n                              position: 'relative',\n                              top: '1px'\n                            }}>\n                              <Button\n                                variant=\"contained\"\n                                color=\"secondary\"\n                                onClick={() => onResetSemester(semester)}\n                                >\n                                Reset\n                              </Button>\n                            </TableCell>\n                            <TableCell colSpan=\"2\" style={{whiteSpace: 'nowrap'}}>\n                              <Button\n                                variant=\"contained\"\n                                color=\"secondary\"\n                                onClick={() => onDeleteSemester(semester)}\n                                startIcon={<DeleteIcon />}\n                                >\n                                Remove semester\n                              </Button>\n                            </TableCell>\n                          </>\n                        }\n                      </TableRow>\n\n                      <SemesterResult\n                        gpa={semester.gpa}\n                        numCourses={numCourses}\n                        semesterNum={semester.number}\n                        onCloseModal={this.props.onCloseModal}\n                      />\n                  </TableBody>\n              </Table>\n          </TableContainer>\n        </div>\n\n        <div id={`semester-${semester.number} clone`} style={{display: 'none'}}>\n          <div style={{\n            fontVariant: 'small-caps',\n            color: 'rgba(249, 71, 47, .9)',\n            textAlign: 'center',\n            border: '2px solid rgba(123, 123, 123, 0.15)',\n            borderRadius: '4px',\n          }}>\n            <h2 style={{margin: '.2rem auto'}}>Semester {semester.number}</h2>\n          </div>\n          <TableContainer component={Paper}>\n            <Table>\n              <TableHead>\n                  <TableRow style={{borderBottom: '1px solid rgb(224, 224, 224)'}}>\n                      <TableCell>Course name</TableCell>\n                      <TableCell>Credit</TableCell>\n                      <TableCell>Marks /100</TableCell>\n                      <TableCell size=\"small\" style={{textAlign: 'center'}}>Grade</TableCell>\n                  </TableRow>\n              </TableHead>\n\n              <TableBody>\n                {semester.courses.map(course =>\n                  <TableRow>\n                    <TableCell>{course.name}</TableCell>\n                    <TableCell>{course.credit}</TableCell>\n                    <TableCell>{course.markOver100}</TableCell>\n                    <TableCell size=\"small\" style={{textAlign: 'center'}}>\n                      {course.grade === '' ? '-' : course.grade}\n                    </TableCell>\n                  </TableRow>\n                )}\n                <TableRow style={{borderTop: '1px solid rgb(224, 224, 224)'}}>\n                  <TableCell colSpan={this.props.numCourses === 1 ? \"4\" : \"5\"} style={{textAlign: 'center'}}>\n                  <Chip\n                    color={setColor(gpa)}\n                    variant=\"outlined\"\n                    label={gpa === 0 ? 'GPA: 0.00 / 4' : `GPA: ${gpa} / 4`}\n                    style={{\n                      fontWeight: '900',\n                      fontSize: '1.1rem',\n                    }}\n                  />\n                  </TableCell>\n                </TableRow>\n              </TableBody>\n            </Table>\n          </TableContainer>\n        </div>\n      </>\n    );\n  }\n}\n"]},"metadata":{},"sourceType":"module"}