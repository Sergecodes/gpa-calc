n          semesterNumber: ''\n        }]\n      }\n    );\n\n    /* Add a course \"form\" to enter course info in the semester */\n    const addCourseInputForm = (semesterNum) => {\n      let semesterConcerned = props.allSemesters.filter(semester => semester.number === semesterNum)[0];\n      if (semesterConcerned === undefined) // if there are no semesters\n          return;\n      let courses = semesterConcerned.courses;\n      let numCourses = courses.length;\n      let newCourseId;\n\n      console.log(semesterConcerned.number === props.number);\n\n      if (numCourses !== 0) {   // if semester already has a course\n          // get unused id by taking id of last course(max id in array) + 1\n          newCourseId = courses[numCourses-1].id + 1;\n\n      }\n      else\n          newCourseId = 1;\n\n      setSemester({\n        ...semesterConcerned,\n        courses: [\n          ...courses, {\n          id: newCourseId,\n          name: '',\n          credit: '',\n          marksOver100: '',\n          grade: '-'\n          }\n        ]\n      });\n\n    }\n\n    const removeCourse = (courseId, semesterNum) => {\n      let semester = props.allSemesters.filter(semester => semester.number === semesterNum)[0];\n      if (semester === undefined) // if there are no semesters\n          return;\n      const remainingCourses = semester.courses.filter(course => course.id !== courseId);\n      setSemester({\n        ...semester,\n        courses: remainingCourses\n      });\n    }\n\n    const handleCourseInfoChange = modifiedCourse => {\n      let semester = props.allSemesters.filter(semester => modifiedCourse.semesterNumber === semesterNum)[0];\n      if (semester === undefined) // if there are no semesters\n          return;\n      const unchangedCourses = semester.courses.filter(course => course.id !== modifiedCourse.id);\n      setSemester({\n        ...semester,\n        courses: unchangedCourses.push(modifiedCourse)\n      });\n      console.log(semester);\n    }\