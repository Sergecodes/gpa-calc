    key={course.id}\n                      course={course}  // pass course to CourseInput component\n                      semester={semester}\n                      onInputChange={onInputChange}\n                      onMarkOrCreditChange={onMarkOrCreditChange}\n                      onDeleteCourse={onDeleteCourse}\n                    />\n                  )\n                )}\n                <TableRow>\n                  <TableCell>\n                    <Button\n                      variant=\"contained\"\n                      color=\"primary\"\n                      onClick={() => onAddCourse(semester)}\n                      startIcon={<AddCircleOutlineIcon />}\n                    >\n                      Add course\n                    </Button>\n                  </TableCell>\n\n                  <TableCell>\n                  </TableCell>\n                  {numSemesters === 1 ?\n                    <>\n                      <TableCell>\n                      </TableCell>\n                      <TableCell style={{paddingLeft: '0'}}>\n                        <Button\n                          variant=\"contained\"\n                          color=\"secondary\"\n                          onClick={() => onResetSemester(semester)}\n                          >\n                          Reset\n                        </Button>\n                      </TableCell>\n                    </>\n                    :\n                    <>\n                      <TableCell style={{\n                        float: 'right',\n                        position: 'relative',\n                        top: '1px'\n                      }}>\n                        <Button\n                          variant=\"contained\"\n                          color=\"secondary\"\n                          onClick={() => onResetSemester(semester)}\n                          >\n                          Reset\n                        </Button>\n                      </TableCell>\n          