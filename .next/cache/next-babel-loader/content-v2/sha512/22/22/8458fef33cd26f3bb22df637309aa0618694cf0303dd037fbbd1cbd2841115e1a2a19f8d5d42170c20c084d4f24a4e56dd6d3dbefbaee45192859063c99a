18n\n    .use(Backend)\n    .use(initReactI18next)\n    .init({\n        lng: lng,\n        backend: {\n            /* translation file path */\n            // ajax: axios,\n            loadPath: '/i18n/translations/{{lng}}.json'\n            // loadPath: `/i18n/translations/${lng}.json`\n        },\n        fallbackLng: 'en',\n        debug: true,\n        //  can have multiple namespace, in case you want to divide a huge translation\n        // into smaller pieces and load them on demand\n        ns: ['translations'],\n        defaultNS: 'translations',\n        keySeparator: false,\n        interpolation: {\n            escapeValue: false,\n            // formatSeparator: ','\n        },\n        react: {\n            useSuspense: false  /* apparently it isn't yet supported by react -_- */\n        }\n    });\n\nexport default i18n;\n"]},"metadata":{},"sourceType":"module"}