\nimport Heading from \"../components/Heading.jsx\";\nimport Semester from \"../components/Semester.jsx\";\nimport { nanoid } from 'nanoid';\n\nexport default function GPACalculator() {\n  const [semesters, setSemesters] = useState([]); // set initial semesters here..\n  console.log(\"rendered semesterss\");\n  const addSemester = () => {\n    let numSemesters = semesters.length;\n    let semesterNum;\n\n    if (numSemesters !== 0)\n        semesterNum = semesters[numSemesters-1].number + 1;\n    else\n        semesterNum = 1;\n    const initialCourses = [{\n      id: '1',\n      name: '',\n      credit: '',\n      marksOver100: '',\n      grade: '-',\n      semesterNumber: semesterNum\n    }, {\n      id: '2',\n      name: '',\n      credit: '',\n      marksOver100: '',\n      grade: '-',\n      semesterNumber: semesterNum\n    }];\n\n    setSemesters([\n      ...semesters,\n      {\n          number: semesterNum,\n          gpa: '-',\n          courses: initialCourses\n      }\n    ]);\n\n  }\n\n  const refreshSemesters = modifiedSemester => {\n    const unmodifiedSemesters = semesters.filter(semester => semester.number !== modifiedSemester.number);\n    unmodifiedSemesters.push(modifiedSemester);\n    const newSemesters = unmodifiedSemesters;\n    setSemesters(newSemesters);\n    console.log(semesters);\n  }\n\n  const removeSemester = num => {\n    const remainingSems = semesters.filter(sem => sem.num !== num);\n    setSemesters(remainingSems);\n  }\n\n  /* return (\n  \t<>\n  \t\t<Semester\n\t\t   number=\"1\"\n\t\t   gpa=\"1.1\"\n       removeSemester={removeSemester}\n       allSemesters={semesters}\n  \t  />\n  \t\t<Semester\n  \t\t   number=\"2\"\n  \t\t   gpa=\"2.2\"\n         removeSemester={removeSemester}\n         allSemesters={semesters}\n    \t/>\n      <button onClick={addSemester}>Add Semester</button>\n\t  </>\n\t); */\n\n  \treturn (\n    \t<>\n        <p>Semesters: {semesters.length}</p>\n    \t\t<Heading />\n    \t\t<br />\n    \t\t<main>\n    