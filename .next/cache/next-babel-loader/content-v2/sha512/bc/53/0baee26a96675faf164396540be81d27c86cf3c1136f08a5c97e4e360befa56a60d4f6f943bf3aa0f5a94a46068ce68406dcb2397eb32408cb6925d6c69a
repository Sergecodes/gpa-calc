Courses(\n      ...unchangedCourses,\n      modifiedCourse\n    );\n  }\n\n  const addSemester = () => {\n    let numSemesters = semesters.length;\n    let semesterNum;\n\n    if (numSemesters !== 0)\n        semesterNum = semesters[numSemesters-1].number + 1;\n    else\n        semesterNum = 1;\n\n    setSemesters(\n      ...semesters,\n      {\n          number: semesterNum,\n          gpa: '-'\n      }\n    );\n  }\n\n  const removeSemester = num => {\n    const remainingSems = semesters.filter(sem => sem.num !== num);\n    setSemesters(remainingSems);\n  }\n\n  console.log(semesters);\n  return (\n    <>\n  \t\t<LanguageSelector />\n      <br />\n  \t\t<Heading />\n  \t\t<br />\n  \t\t<main>\n        {semesters.map(\n          (semester) => (\n            <>\n              <Semester\n                key={semester.number}\n                allCourses={allCourses}\n                addCourseInput={addCourseInput}\n                removeSemester={removeSemester}\n                removeCourse={removeCourse}\n                handleCourseInfoChange={handleCourseInfoChange}\n              />\n              <br />\n            </>\n          )\n        )}\n        <button onClick={addSemester}>Add Semester</button>\n  \t\t</main>\n    </>\n  );\n}\n"]},"metadata":{},"sourceType":"module"}